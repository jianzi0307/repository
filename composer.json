{
    "name": "jianzi0307/repository",
    "description": "laravel repository pattern.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jianzi0307",
            "email": "jianzi0307@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Jianzi\\Repository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jianzi\\Repository\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jianzi\\Repository\\RepositoryServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "scripts-descriptions": {
        "test": "Run all tests."
    }
}
